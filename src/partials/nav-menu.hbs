{{#if (eq page.attributes.theme 'cheat-sheet') }}
  <aside class="toc sidebar" data-title="{{or page.attributes.toctitle ''}}"
  data-levels="{{{or page.attributes.toclevels 2}}}">
  <div class="toc-menu">
    <div class="toc-menu-placeholder"></div>
  </div>
</aside>
{{else}}
<div class="nav-panel-menu is-active" data-panel="menu">
    <nav class="nav-menu">

      

      {{!-- add link back to home --}}
      {{#unless page.home}}
      {{#if page.attributes.show-home-link}}
      <ul class="nav-list">
        <li class="nav-item home-link" data-depth="1" aria-label="home-link">
          {{#with site.homeUrl}}
          <a href="{{{relativize this}}}" aria-label="Go to docs home page">Docs home</a>
          {{/with}}
        </li>
      </ul>
      {{/if}}
      {{/unless}}


    {{#with page.navigation}}{{> nav-tree navigation=this}}{{/with}}
  </nav>
</div>
{{/if}}